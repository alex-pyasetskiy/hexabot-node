{
  "name": "react-hexapod-sim",
  "version": "0.1.0",
  "private": true,
  "module": "commonjs",
  "lib": [
    "ES6"
  ],
  "isolatedModules": true,
  "strict": true,
  "noEmit": true,
  "homepage": "https://alex-pyasetskiy.github.io/react-hexapod-simulator/",
  "dependencies": {
    "colors": "^1.4.0",
    "drivelist": "^9.1.0",
    "express": "^4.17.1",
    "node-hid": "^1.3.1",
    "node-usb-detection": "^1.0.2",
    "serialport": "^9.0.0",
    "usb-detection": "^4.10.0",
    "websocket": "^1.0.31"
  },
  "scripts": {
    "start": "node src/index.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json"
  ]
}
